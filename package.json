{
	"name": "generate-emoji-list",
	"version": "1.0.0",
	"description": "Generate a comprehensive list of all Unicode Emojis in any language.",
	"keywords": [
		"emoji",
		"unicode-emoji",
		"cloud-translation-api",
		"json",
		"emoji-list",
		"generate-emoji-list"
	],
	"license": "MIT",
	"author": "Leonhard Driesch",
	"sideEffects": false,
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"generate-emoji-list": "dist/cli.js"
	},
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "tsc",
		"check-types": "tsc --noEmit",
		"format": "prettier . --write",
		"lint": "eslint . --ignore-path .gitignore",
		"prepublishOnly": "yarn test && yarn build",
		"start": "ts-node src/cli.ts",
		"test": "run-p lint check-types"
	},
	"dependencies": {
		"@google-cloud/translate": "^6.0.3",
		"enquirer": "^2.3.6",
		"fs-extra": "^9.0.1",
		"isomorphic-unfetch": "^3.1.0",
		"tslib": "^2.0.3"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.3",
		"@types/node": "^14.14.7",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-config-standard-with-typescript": "^19.0.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"prettier-plugin-packagejson": "^2.2.7",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5"
	},
	"engines": {
		"node": ">=14"
	}
}
